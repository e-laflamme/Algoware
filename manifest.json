{
  "manifest_version": 3,
  "name": "YouTube Title Scraper",
  "version": "1.0",
  "description": "Extracts YouTube homepage video titles",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://www.youtube.com/"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/"],
      "js": ["content.js"]
    }
  ]
}